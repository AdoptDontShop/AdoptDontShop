<h1>User's profile</h1>

<h3>My Pets:</h3>
<% current_user.pets.each do |pet| %>
	<p><%= pet.name %><p>
<% end %>

<% if Favorite.where(:user_id => current_user.id).first %>

<h3>My Favorited Pets:</h3>
<% f = Favorite.where(:user_id => current_user.id)  %>
	<% f.each do |fav| %>
		<p>
		<%= Pet.where(:id => fav[:pet_id]).first[:name] %>
		</p>
	<% end %>
<% end %>