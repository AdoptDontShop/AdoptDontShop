<%= simple_form_for :pets do |p| %>
	<%= p.input :name, placeholder: 'Enter name' %>
	<%= p.input :type %>
	<%= p.input :activity_level %>
	<%= p.input :personality %>
	<%= p.label :size %>
	<%= p.select :size, [['Big','big'],['Medium','medium'],['Small','small']] %>
	<%= p.button :submit %>
<% end %>
